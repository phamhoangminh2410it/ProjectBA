<script>
    var texts = document.querySelectorAll('.master.text');

    for (var z = 0; z < texts.length; z++) {
        var text = texts[z];
        //var words = text.textContent.trim().split(" ");
        var noiDung = text.textContent.replace(/\n/g, " \n ");
        var words = noiDung.trim().split(" ");
        words = words.filter(function (w) {
            return w != "";
        });
        text.textContent = '';
        text.innerHTML = '&nbsp;';
        var heightBangDau = text.clientHeight;
        var beforeBreak = '';
        for (var i = 0; i < words.length; i++) {
            text.textContent += ' ' + words[i];
            if (text.clientHeight > heightBangDau * 1.5 || words[i] == '\n') {
                text.textContent = beforeBreak;
                beforeBreak = words[i];
                var haveNextText = false;
                if (text.nextElementSibling) {
                    if (text.nextElementSibling.classList.contains("text")) {
                        haveNextText = true;
                    }
                }
                if (haveNextText) {
                    var nextText = text.nextElementSibling;
                    nextText.style.display = 'block';
                    text.insertAdjacentElement('afterend', nextText);
                    text = nextText;
                    text.textContent = words[i];
                } else {
                    var haveParentFlex = checkParentFlex(text);
                    var nextText = document.createElement('p');
                    nextText.className = text.className.replace('master', '');
                    nextText.style.display = 'block';
                    nextText.style.width = text.clientWidth;
                    if (haveParentFlex) {
                        var haveNextPOfParentFlex = false;
                        if (haveParentFlex.nextElementSibling) {
                            if (haveParentFlex.nextElementSibling.classList.contains("text")) {
                                haveNextPOfParentFlex = true;
                            }
                        }
                        if (haveNextPOfParentFlex) {
                            nextText = haveParentFlex.nextElementSibling;
                            nextText.style.display = 'block';
                            haveParentFlex.insertAdjacentElement('afterend', nextText);
                        } else {
                            haveParentFlex.insertAdjacentElement('afterend', nextText);
                        }
                    } else {
                        text.insertAdjacentElement('afterend', nextText);
                    }
                    text = nextText;
                    text.textContent = words[i];
                }
            } else {
                beforeBreak += ' ' + words[i];
            }
        }
    }
    function checkParentFlex(teDiv) {
        // Khởi tạo biến để lưu trữ phần tử cha hiện tại
        var currentElement = teDiv.parentElement;
        var foundFlexClass = false;

        // Duyệt qua tất cả các phần tử cha của phần tử có id là "te"
        while (currentElement) {
            // Kiểm tra xem phần tử cha hiện tại có class 'flex' không
            if (currentElement.classList.contains('flex')) {
                foundFlexClass = currentElement;
                break;
            }
            // Di chuyển lên phần tử cha tiếp theo
            currentElement = currentElement.parentElement;
        }
        return foundFlexClass;
    }
</script>